{
  "name": "kanel",
  "version": "2.0.0",
  "keywords": [
    "postgresql",
    "schema",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristiandupont/kanel.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kristiandupont/kanel/issues"
  },
  "homepage": "https://github.com/kristiandupont/kanel#readme",
  "author": {
    "name": "Kristian Dupont",
    "url": "http://www.kristiandupont.com"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "kanel": "./bin/kanel"
  },
  "files": [
    "build/",
    "bin/"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "yarn eslint . -f visualstudio --report-unused-disable-directives --ext .js,.ts",
    "run-example": "node build/cli -c ./example/.kanelrc",
    "start-example-db": "docker run -d --name dvd-rental -p 54321:5432 kristiandupont/dvdrental-image",
    "stop-example-db": "docker stop dvd-rental",
    "test": "yarn jest",
    "version": "yarn build"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.(js|ts)"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "ts-jest"
    }
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "extract-pg-schema": "^3.0.0",
    "optionator": "^0.9.1",
    "pg": "^8.6.0",
    "ramda": "^0.27.1",
    "rmfr": "^2.0.0"
  },
  "devDependencies": {
    "@kristiandupont/recase": "1.1.1",
    "@types/jest": "26.0.23",
    "@types/node": "14.14.43",
    "@types/ramda": "0.27.40",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "26.6.3",
    "np": "7.5.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "np": {}
}
